{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_dev_server;Database=your_dev_database;User Id=your_dev_user;Password=your_dev_password;Encrypt=True;TrustServerCertificate=True;"
  },
  "Jwt": {
    "Issuer": "your_dev_issuer_url",
    "Audience": "your_dev_audience_url",
    "Key": "your_dev_jwt_secret_key_minimum_32_characters"
  }
}